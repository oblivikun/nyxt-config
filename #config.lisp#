(define-configuration (input-buffer)
    ((default-modes (remove-if (lambda (nyxt::m) (find (symbol-name nyxt::m) '("EMACS-MODE" "VI-NORMAL-MODE" "VI-INSERT-MODE") :test #'string=)) %slot-value%)))) (define-configuration (input-buffer) ((default-modes (pushnew 'nyxt/mode/emacs:emacs-mode %slot-value%))))
